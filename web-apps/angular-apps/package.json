{
  "name": "@cloud77/angular-apps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000 --proxy-config ./proxy.json",
    "start-sample": "ng serve --project sample-app --port 4100 --proxy-config ./proxy.json",
    "start-account": "ng serve --project account-app --port 4200 --proxy-config ./proxy.json",
    "start-dashboard": "ng serve --project dashboard-app --port 4300 --proxy-config ./proxy.json",
    "start-internal": "ng serve --project internal-app --port 4400 --proxy-config ./proxy.json",
    "build": "ng build",
    "build-sample": "ng build sample-app --configuration production --aot --source-map=false",
    "build-landing": "ng build landing-app --configuration production --aot --source-map=false",
    "build-account": "ng build account-app --configuration production --aot --source-map=false",
    "build-dashboard": "ng build dashboard-app --configuration production --aot --source-map=false",
    "build-internal": "ng build internal-app --configuration production --aot --source-map=false",
    "build-all": "yarn build-sample && yarn build-landing && yarn build-account && yarn build-dashboard && yarn build-internal",
    "build-gh-page": "ng build --project gh-page-app",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.8",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.6",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@microsoft/signalr": "^8.0.7",
    "@ng-util/monaco-editor": "^18.0.0",
    "@phantom-chen/cloud77": "^1.0.3",
    "@swimlane/ngx-charts": "^20.5.0",
    "@zxcvbn-ts/core": "^3.0.0",
    "@zxcvbn-ts/language-en": "^3.0.0",
    "angular-calendar": "^0.31.1",
    "angular-gridster2": "^18.0.1",
    "angular-password-strength-meter": "^12.0.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.6",
    "guid-typescript": "^1.0.9",
    "html2canvas": "^1.4.1",
    "jquery": "^3.7.1",
    "marked": "^14.1.2",
    "md5-typescript": "^1.0.5",
    "monaco-editor": "^0.52.0",
    "ng2-charts": "^6.0.1",
    "ngx-markdown": "^19.0.0",
    "ngx-socket-io": "^4.8.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "sigma": "^3.0.0-beta.32",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "^18.2.5",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  },
